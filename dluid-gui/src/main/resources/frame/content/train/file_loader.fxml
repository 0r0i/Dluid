<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="itself" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>

        <!--============================================================= Titled =============================================================-->
        <TitledPane fx:id="titledPane" animated="false" text="학습 파라미터" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <GridPane hgap="5.0" vgap="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="150" minWidth="130" prefWidth="140" />
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="80" prefWidth="240" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>

                                <!--============================================================= Training Feature file finder =============================================================-->
                                <Label fx:id="labelTrainingTarget" text="Target input : " GridPane.rowIndex="0" />
                                <TextField fx:id="textFieldTrainingTarget" disable="true" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="0">
                                    <padding>
                                        <Insets bottom="5.0" top="5.0" />
                                    </padding>
                                </TextField>

                                <Label fx:id="labelTrainingFeature" text="Feature : " GridPane.rowIndex="1" />
                                <GridPane hgap="5.0" vgap="5.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" />
                                        <ColumnConstraints hgrow="NEVER" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <AnchorPane GridPane.columnIndex="0">
                                            <TextField fx:id="textFieldFindTrainingFeature" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                        </AnchorPane>
                                        <AnchorPane GridPane.columnIndex="1">
                                            <Button fx:id="buttonFindTrainingFeature" styleClass="customDefaultButton" text="Browse ..." AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            </Button>
                                        </AnchorPane>
                                    </children>
                                    <padding>
                                        <Insets bottom="5.0" top="5.0" />
                                    </padding>
                                </GridPane>

                                <!--============================================================= Training Result file finder =============================================================-->
                                <Label fx:id="labelTrainingResult" text="Result : " GridPane.rowIndex="2" />
                                <GridPane hgap="5.0" vgap="5.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" />
                                        <ColumnConstraints hgrow="NEVER" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <AnchorPane GridPane.columnIndex="0">
                                            <TextField fx:id="textFieldFindTrainingResult" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                        </AnchorPane>
                                        <AnchorPane GridPane.columnIndex="1">
                                            <Button fx:id="buttonFindTrainingResult" styleClass="customDefaultButton" text="Browse ..." AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            </Button>
                                        </AnchorPane>
                                    </children>
                                    <padding>
                                        <Insets bottom="5.0" top="5.0" />
                                    </padding>
                                </GridPane>

                            </children>
                     <padding>
                        <Insets bottom="10.0" />
                     </padding>
                        </GridPane>
                        <Pagination fx:id="paginationFileLoader" pageCount="1" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin></Pagination>
                    </VBox>
                </AnchorPane>
            </content>
            <padding>
                <Insets bottom="20.0" />
            </padding>
        </TitledPane>

    </children>
</AnchorPane>
