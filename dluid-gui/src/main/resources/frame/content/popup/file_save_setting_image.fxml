<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="itself" stylesheets="@../../../css/common.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>

        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>

                <GridPane hgap="5.0" vgap="5.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label fx:id="labelName" text="Image name : " GridPane.rowIndex="0" />
                        <Label fx:id="labelWidth" text="Image width : " GridPane.rowIndex="1" />
                        <Label fx:id="labelHeight" text="Image height : " GridPane.rowIndex="2" />
                        <Label fx:id="labelColorType" text="Image type : " GridPane.rowIndex="3" />
                        <TextField fx:id="textFieldName" promptText="Image name" text="Result" GridPane.columnIndex="1" GridPane.rowIndex="0">
                            <padding>
                                <Insets bottom="5.0" top="5.0" />
                            </padding>
                        </TextField>
                        <TextField fx:id="textFieldWidth" promptText="28" text="1" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <padding>
                                <Insets bottom="5.0" top="5.0" />
                            </padding>
                        </TextField>
                        <TextField fx:id="textFieldHeight" promptText="28" text="1" GridPane.columnIndex="1" GridPane.rowIndex="2">
                            <padding>
                                <Insets bottom="5.0" top="5.0" />
                            </padding>
                        </TextField>
                        <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                            <children>
                                <MenuButton fx:id="menuButtonColorType" styleClass="customMenuButton" mnemonicParsing="false" text="" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <items>
                                    </items>
                                </MenuButton>
                            </children>
                        </AnchorPane>
                    </children>
                    <VBox.margin>
                        <Insets bottom="15.0" />
                    </VBox.margin>
                </GridPane>



                <HBox alignment="TOP_RIGHT">
                    <children>
                        <Button fx:id="buttonSaveData" styleClass="customGreenButton" maxWidth="100.0" minWidth="10.0" prefHeight="30.0" prefWidth="100.0" text="Save">
                             <HBox.margin>
                                <Insets right="10.0" />
                             </HBox.margin>
                        </Button>
                        <Button fx:id="buttonCancel" styleClass="customRedButton" maxWidth="100.0" minWidth="10.0" prefHeight="30.0" prefWidth="100.0" text="Cancel">
                        </Button>
                    </children>
                </HBox>

            </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
        </VBox>
    </children>
</AnchorPane>
