<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="itself" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ScrollPane fitToWidth="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <content>
                <VBox fx:id="content" prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                    </padding>
                    <children>
                        <Label fx:id="labelTestDataSetting" text="1. Test data setting">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets bottom="10.0" />
                     </padding></Label>
                        <VBox fx:id="vBoxForTestData">
                     <padding>
                        <Insets bottom="10.0" />
                     </padding></VBox>
                        <Label fx:id="labelTestTask" text="2. Test">
                     <padding>
                        <Insets bottom="10.0" />
                     </padding></Label>
                        <VBox fx:id="vBoxForTestTask">
                     <padding>
                        <Insets bottom="10.0" />
                     </padding></VBox>
                    </children>
                </VBox>

            </content>
        </ScrollPane>
    </children>
</AnchorPane>
