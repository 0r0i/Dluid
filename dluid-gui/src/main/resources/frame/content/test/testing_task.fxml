<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXProgressBar?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="itself" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>

        <!--============================================================= Titled =============================================================-->
        <TitledPane fx:id="titledPane" animated="false" text="Test" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                    <children>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                             <children>
                                 <GridPane hgap="5.0" vgap="5.0">
                                     <columnConstraints>
                                         <ColumnConstraints hgrow="SOMETIMES" maxWidth="150" minWidth="130" prefWidth="140" />
                                         <ColumnConstraints hgrow="SOMETIMES" minWidth="80" prefWidth="240" />
                                     </columnConstraints>
                                     <rowConstraints>
                                         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                     </rowConstraints>
                                     <children>
                                         <Label fx:id="labelTestTargetResult" text="Target extracting layer id : " GridPane.columnIndex="0" GridPane.rowIndex="0" />
                                         <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0">
                                             <children>
                                                 <MenuButton fx:id="menuButtonTestTargetResultLayer" styleClass="customMenuButton" mnemonicParsing="false" text="" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                     <items>
                                                     </items>
                                                 </MenuButton>
                                             </children>
                                             <padding>
                                                 <Insets bottom="5.0" top="5.0" />
                                             </padding>
                                         </AnchorPane>
                                     </children>
                                     <VBox.margin>
                                         <Insets bottom="10.0" />
                                     </VBox.margin>
                                 </GridPane>


                                 <AnchorPane>
                                     <children>
                                         <TextArea fx:id="textAreaTestLog" maxHeight="150.0" minHeight="150.0" prefHeight="150.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                     </children>
                                     <padding>
                                         <Insets bottom="10.0" />
                                     </padding>
                                 </AnchorPane>
                                 <AnchorPane>
                                     <children>
                                         <JFXProgressBar fx:id="progressBarTestProgress" progress="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                     </children>
                                     <VBox.margin>
                                         <Insets />
                                     </VBox.margin>
                                     <padding>
                                         <Insets bottom="10.0" />
                                     </padding>
                                 </AnchorPane>


                                 <HBox alignment="TOP_RIGHT" prefWidth="200.0">
                                     <children>
                                         <Button fx:id="buttonTest" mnemonicParsing="false" styleClass="customDefaultButton" text="Test">
                                             <HBox.margin>
                                                 <Insets left="5.0" />
                                             </HBox.margin>
                                         </Button>
                                     </children>
                                     <VBox.margin>
                                         <Insets />
                                     </VBox.margin>
                                 </HBox>
                             </children>
                        </VBox>
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>

    </children>
</AnchorPane>
