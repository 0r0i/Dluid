<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="itself" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <!--============================================================= Titled =============================================================-->
        <TitledPane fx:id="titledPane" animated="false" text="Test data random generator" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                    <children>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <children>
                                <GridPane>
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="150" minWidth="10.0" prefWidth="100.0" />
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="0">
                                            <children>
                                                <Label fx:id="labelRandomMin" alignment="CENTER" text="Min" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                            </children>
                                        </AnchorPane>
                                        <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="0">
                                            <children>
                                                <Label fx:id="labelRandomMax" alignment="CENTER" text="Max" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                            </children>
                                        </AnchorPane>
                                        <Label fx:id="labelRandom" text="Random range : " GridPane.columnIndex="0" GridPane.rowIndex="1" />
                                        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                                            <children>
                                                <TextField fx:id="textFieldRandomInputRangeMin" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                </TextField>
                                            </children>
                                        </AnchorPane>
                                        <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="1">
                                            <children>
                                                <TextField fx:id="textFieldRandomInputRangeMax" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                </TextField>
                                            </children>
                                        </AnchorPane>
                                    </children>
                                </GridPane>





                                <GridPane vgap="5.0">
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="100.0" />
                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                        <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                        <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <Label fx:id="labelRandomInputSize" text="Input size : " GridPane.columnIndex="0" GridPane.rowIndex="0" />
                                        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="0">
                                            <children>
                                                <TextField fx:id="textFieldRandomInputSize" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                </TextField>
                                            </children>
                                        </AnchorPane>
                                        <Label fx:id="labelRandomRecordNumber" text="Record number : " GridPane.columnIndex="0" GridPane.rowIndex="1" />
                                        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                                            <children>
                                                <TextField fx:id="textFieldRandomRecordNumber" promptText="5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                </TextField>
                                            </children>
                                        </AnchorPane>

                                        <Label fx:id="labelRandomDataType" text="Integer : " GridPane.columnIndex="0" GridPane.rowIndex="2" />
                                        <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="2">
                                            <children>
                                                <HBox alignment="CENTER_LEFT">
                                                    <RadioButton fx:id="radioButtonIntegerType" selected="true" text="Integer" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                        <toggleGroup>
                                                            <ToggleGroup fx:id="toggleGroupDataType" />
                                                        </toggleGroup>
                                                        <padding>
                                                            <Insets right="20.0" />
                                                        </padding>
                                                    </RadioButton>
                                                    <RadioButton fx:id="radioButtonDoubleType" selected="false" text="Float" toggleGroup="$toggleGroupDataType" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                        <padding>
                                                            <Insets right="20.0" />
                                                        </padding>
                                                    </RadioButton>
                                                    <RadioButton fx:id="radioButtonGaussianType" selected="false" text="Gaussian" toggleGroup="$toggleGroupDataType" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                    </RadioButton>
                                                </HBox>
                                            </children>
                                        </AnchorPane>
                                    </children>
                                    <VBox.margin>
                                        <Insets top="5.0" />
                                    </VBox.margin>
                                </GridPane>




                                <HBox alignment="CENTER_RIGHT">
                                    <Button fx:id="buttonGenerateRandomData" text="Generate" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    </Button>
                                    <padding>
                                        <Insets top="10.0" />
                                    </padding>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>

    </children>
</AnchorPane>
