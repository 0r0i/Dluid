<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="itself" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TitledPane fx:id="titledPane" animated="false" text="Layer common information" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                    <children>
                        <!-- start content-->
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <GridPane vgap="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="150" minWidth="10.0" prefWidth="100.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                                <children>
                                    <Label fx:id="labelLayerId" text="Layer ID" GridPane.rowIndex="1" />
                                    <Label fx:id="labelLayerType" text="Layer Type" GridPane.rowIndex="0" />
                                    <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="0">
                                        <children>
                                            <TextField fx:id="textFieldLayerId" promptText="123" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" disable="true" editable="false">
                                            </TextField>
                                        </children>
                                    </AnchorPane>
                                    <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                                        <children>
                                            <TextField fx:id="textFieldLayerType" promptText="123" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" disable="true" editable="false">
                                            </TextField>
                                        </children>
                                    </AnchorPane>
                                </children>
                        <VBox.margin>
                           <Insets bottom="5.0" />
                        </VBox.margin>
                            </GridPane>
                            <HBox alignment="CENTER_RIGHT">
                                <Button fx:id="buttonDelete"  text="Delete" />
                            </HBox>
                        </VBox>
                        <!-- end content-->
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
    </children>
</AnchorPane>
