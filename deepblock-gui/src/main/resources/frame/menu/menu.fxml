<?import javafx.scene.control.*?>
<MenuBar xmlns="http://javafx.com/javafx/8.0.172-ea"
         xmlns:fx="http://javafx.com/fxml/1"
         fx:id="itself">
    <menus>
        <Menu fx:id="menuFile" text="파일">
            <items>
                <MenuItem fx:id="menuFileNew" text="새로 만들기" />
                <MenuItem fx:id="menuFileModelLoad" text="모델 불러오기" />
                <MenuItem fx:id="menuFileModelSave" text="모델 저장하기" />
                <MenuItem fx:id="menuFileExit" text="끝내기" />
            </items>
        </Menu>
        <Menu fx:id="menuSetting" text="설정">
            <items>
                <MenuItem fx:id="menuSettingCanvas" text="캔버스" />
            </items>
        </Menu>
        <Menu fx:id="menuAdvanced" text="고급">
            <items>
                <MenuItem fx:id="menuAdvancedChange" text="고급 모드로 전환" />
            </items>
        </Menu>
    </menus>
</MenuBar>
