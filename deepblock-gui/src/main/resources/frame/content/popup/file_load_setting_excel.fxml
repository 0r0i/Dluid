<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="itself" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <CheckBox fx:id="checkBoxHeaderExist" mnemonicParsing="false" selected="true" text="헤더 유무">
                    <VBox.margin>
                        <Insets bottom="5.0" />
                    </VBox.margin>
                </CheckBox>
                <GridPane hgap="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label fx:id="labelDataStart" text="데이터 시작 : " GridPane.columnIndex="0" />
                        <Label fx:id="labelDataEnd" text="데이터 마지막 : " GridPane.rowIndex="1" />
                        <TextField fx:id="textFieldDataStart" promptText="A1" text="A1" GridPane.columnIndex="1">
                            <padding>
                                <Insets bottom="5.0" top="5.0" />
                            </padding>
                        </TextField>
                        <TextField fx:id="textFieldDataEnd" promptText="C10" text="A1" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <padding>
                                <Insets bottom="5.0" top="5.0" />
                            </padding>
                        </TextField>
                    </children>
                    <VBox.margin>
                        <Insets bottom="5.0" />
                    </VBox.margin>
                </GridPane>





                <HBox alignment="TOP_RIGHT">
                    <children>
                        <Button fx:id="buttonLoadData" maxWidth="100.0" minWidth="10.0" prefHeight="30.0" prefWidth="100.0" text="Load">
                            <HBox.margin>
                                <Insets right="10.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="buttonCancel" maxWidth="100.0" minWidth="10.0" prefHeight="30.0" prefWidth="100.0" text="Cancel">
                        </Button>
                    </children>
                </HBox>

            </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
        </VBox>

    </children>
</AnchorPane>
