<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="itself" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TitledPane fx:id="tiltedPane" animated="false" text="레이어 함수" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                    <children>
                        <!-- start content-->
                        <GridPane vgap="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="150" minWidth="10.0" prefWidth="100.0" />
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <Label fx:id="labelWeightInitializer" text="초기화 함수" GridPane.rowIndex="0" />
                                <Label fx:id="labelActivationFunction" text="활성화 함수" GridPane.rowIndex="1" />
                                <Label fx:id="labelDropout" text="망각 비율" GridPane.rowIndex="2" />
                                <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="0">
                                    <children>
                                        <SplitMenuButton fx:id="splitMenuWeightInit" mnemonicParsing="false" text="" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <items>
                                            </items>
                                        </SplitMenuButton>
                                    </children>
                                </AnchorPane>
                                <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                                    <children>
                                        <SplitMenuButton fx:id="splitMenuActivationFunction" mnemonicParsing="false" text="" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <items>
                                            </items>
                                        </SplitMenuButton>
                                    </children>
                                </AnchorPane>
                                <AnchorPane GridPane.columnIndex="1" GridPane.rowIndex="2">
                                    <children>
                                        <Slider fx:id="sliderDropout" max="1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                        </Slider>
                                    </children>
                                </AnchorPane>
                            </children>
                        </GridPane>
                        <!-- end content-->
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
    </children>
</AnchorPane>
